import java.util.Arrays;

public class matris {
    public static void main(String[] args) {
        int[][] m={ {1,2,3}, {4,5,6}, {7,8,9}};
        int[][] n=new int[3][3];

      for(int i=0;i<m.length;i++){
          for (int j=0;j<m[0].length;j++){
              n[i][j]=m[j][i];
              System.out.print(n[i][j]+" ");
          }
          System.out.println();
          }

    }
}
