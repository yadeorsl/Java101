public class asalsayiyazdir {
    public static void main(String[] args) {
        int i,n,sayac;
        sayac=0;
        for (n=2;n<=100;n++){
            int kontrol=0;
            for(i=2;i<n;i++){
            if(n%i==0){
                kontrol++;
                break;
            }   }
            if(kontrol==0){
                System.out.println(n);
                sayac++;
            }

    }
}    }
