import java.util.Scanner;

public class harmonik {
    public static void main(String[] args) {
        double result=0;
        Scanner inp=new Scanner(System.in);
        System.out.println("sayi gir");
        int n= inp.nextInt();
        for (int i=1;i<=n;i++){
            result+=(1.0/i);
        }
        System.out.println(result);
    }
}
